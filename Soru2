List<int> tekSayilariFiltrele(List<int> liste) {
  List<int> tekSayilar = [];
  for (int sayi in liste) {
    if (sayi % 2 != 0) {
      tekSayilar.add(sayi);
    }
  }
  return tekSayilar;
}


void main() {
  List<int> sayilar = [1, 2, 3, 4, 5, 6, 7, 8, 9];
  print(tekSayilariFiltrele(sayilar));
}
